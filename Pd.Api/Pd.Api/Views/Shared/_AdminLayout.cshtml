    <script src="~/Script/assets/js/jquery-1.10.2.js"></script>
    <!-- BOOTSTRAP SCRIPTS -->
    <script src="~/Script/assets/js/bootstrap.js"></script>
    <!-- METISMENU SCRIPTS -->
