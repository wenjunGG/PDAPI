                    </li>
                    <li>
                        <a href="#"><i class="fa fa-desktop "></i>用户管理 <span class="fa arrow"></span></a>
                        <ul class="nav nav-second-level">
                            <li>
                                <a href="/Admin/User/Index"><i class="fa fa-toggle-on"></i>用户管理</a>
                            </li>
                            <li>
                                <a href="/Admin/UserModular/Index"><i class="fa fa-toggle-on"></i>用户模板管理</a>
                            </li>
                        </ul>
                    </li>
                    <li>
                        <a href="#"><i class="fa fa-yelp "></i>模块 <span class="fa arrow"></span></a>
                        <ul class="nav nav-second-level">
                            <li>
                                <a href="/Admin/Modular/Index"><i class="fa fa-coffee"></i>模块管理</a>
                            </li>
                            <li>
                                <a href="/Admin/DicModular/Index"><i class="fa fa-coffee"></i>模块字典管理</a>
                            </li>
                            <li>
                                <a href="/Admin/Head/Index"><i class="fa fa-coffee"></i>Header管理</a>
                            </li>
                        </ul>
                    </li>
                    <li>
                        <a href="#"><i class="fa fa-yelp "></i>接口 <span class="fa arrow"></span></a>
                        <ul class="nav nav-second-level">
                            <li>
                                <a href="/Admin/Ports/Index"><i class="fa fa-coffee"></i>接口管理</a>
                            </li>
                            <li>
                                <a href="/Admin/DicPorts/Index"><i class="fa fa-coffee"></i>接口字典管理</a>
                            </li>
                            <li>
                                <a href="/Admin/Coefficient/Index"><i class="fa fa-coffee"></i>参数管理</a>
                            </li>
                            <li>
                                <a href="/Admin/RequestCoeff/Index"><i class="fa fa-coffee"></i>请求参数管理</a>
                            </li>
                            <li>
                                <a href="/Admin/Back/Index"><i class="fa fa-coffee"></i>返回值管理</a>
                            </li>
                        </ul>
                    </li>
                </ul>

            </div>

        </nav>


        <!-- /. NAV SIDE  -->
        <div id="page-wrapper">
            <div id="page-inner">
                <div class="row">
                    <div class="col-md-12">
                        <h5 class="page-head-line">@ViewBag.tag</h5>
                    </div>
                </div>
                <div>
                    @RenderBody()
                </div>


            </div>
            <!-- /. PAGE INNER  -->
        </div>
        <!-- /. PAGE WRAPPER  -->
    </div>
    <!-- /. WRAPPER  -->

    <div id="footer-sec">
        &copy; 2014 YourCompany | Design By : <a href="http://www.binarytheme.com/" target="_blank">BinaryTheme.com</a>
    </div>
    <!-- /. FOOTER  -->
    <!-- SCRIPTS -AT THE BOTOM TO REDUCE THE LOAD TIME-->
